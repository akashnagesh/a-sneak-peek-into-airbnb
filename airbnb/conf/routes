# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

GET     /design                           controllers.HomeController.designStrategy

POST    /login                      controllers.HomeController.userLogin

POST    /signUp                     controllers.HomeController.createUser

GET      /echo                       controllers.HomeController.echo

GET         /graph                      controllers.UserController.graph

GET         /graph1                      controllers.UserController.graph1


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
